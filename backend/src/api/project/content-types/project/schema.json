{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "project_title": {
      "type": "string",
      "required": true
    },
    "project_images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "project_type": {
      "type": "enumeration",
      "enum": [
        "software",
        "website",
        "app"
      ]
    },
    "industry": {
      "type": "enumeration",
      "enum": [
        "agriculture & farming",
        "banking & financial services",
        "construction & manufacturing",
        "education & training",
        "energy & utilities",
        "entertainment",
        "environmental & waste management",
        "government & public sector",
        "health & wellness",
        "hospitality",
        "insurance",
        "legal & compliance",
        "media & broadcasting",
        "nonprofit & ngos",
        "pharmaceuticals & biotechnology",
        "real estate & property management",
        "retail & consumer goods",
        "technology",
        "telecommunications",
        "transportation & logistics"
      ],
      "required": true
    },
    "project_description": {
      "type": "text",
      "required": false
    },
    "project_link": {
      "type": "string"
    },
    "project_date": {
      "type": "date"
    },
    "project_client": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::client.client",
      "inversedBy": "projects"
    },
    "thumbnail_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
